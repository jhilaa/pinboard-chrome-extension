{
  "manifest_version": 3,
  "name": "Pinboard Wizard",
  "version": "1.0",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "cookies",
    "sidePanel",
    "webNavigation",
    "https://api.airtable.com/"
  ],
  "side_panel": {
    "default_path": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "./public/icone16.png",
      "48": "./public/icone48.png",
      "128": "./public/icone128.png"
    }
  },
  "icons": {
    "16": "./public/run16.png",
    "48": "./public/run48.png",
    "128": "./public/run128.png"
  }
}
